{
  "name": "better-fetch",
  "version": "1.0.0",
  "description": "A project to implement advanced service worker functionality like pulling from cache and then recieving network data and updating the display to show newer data into a wrapper with standard fetch calls.",
  "main": "better-fetch.js",
  "scripts": {
    "test": "./node_modules/.bin/istanbul cover jest",
    "serve": "node tests/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tibblist/better-fetch.git"
  },
  "keywords": [
    "service-worker",
    "fetch",
    "api",
    "cache"
  ],
  "author": "Ethan Tibbetts",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Tibblist/better-fetch/issues"
  },
  "homepage": "https://github.com/Tibblist/better-fetch#readme",
  "devDependencies": {
    "express": "^4.17.1",
    "istanbul": "^0.4.5",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-puppeteer": "^4.2.0",
    "node-fetch": "^2.6.0",
    "puppeteer": "^1.17.0",
    "webpack": "^4.33.0",
    "webpack-dev-middleware": "^3.7.0"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "globals": {
      "PATH": "http://localhost:4444"
    },
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
